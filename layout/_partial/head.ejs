<!--[if lte IE 9]>
<meta http-equiv="refresh" content="0;url=<%= config.url %>/warn.html">
<![endif]-->
<meta charset="utf-8">
<% if (theme.fullHttps){ %>
<meta http-equiv="Content-Security-Policy" content="upgrade-insecure-requests">
<% } %>
<% if (theme.google_site_verification){ %>
<meta http-equiv="Content-Security-Policy" content="<%= theme.google_site_verification %>">
<% } %>
<meta http-equiv="X-UA-Compatible" content="IE=Edge">
<meta name="renderer" content="webkit">
<meta name="viewport" content="width=device-width, initial-scale=1.0,user-scalable=no">
<meta http-equiv="mobile-agent" content="format=html5; url=<%= config.url %>">
<meta name="author" content="<%= config.author %>">
<%- css('//netdna.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css') %>
<%- css(['/css/SimpleStyle.css']) %>
<%- css(['/css/custom.css']) %>
<% if (is_post()){ %>
<%- css(['/css/highlight/highlight.css']) %>
<% } %>
<% if (theme.favicon){ %>
<%- favicon_tag(theme.favicon) %>
<% } %>
<%
var siteTitle = page.title;
if (is_archive()) {
    siteTitle = __('archives');
    if (is_month()) {
        siteTitle += '- ' + page.year + '/' + page.month;
    } else if (is_year()) {
        siteTitle += '- ' + page.year;
    }
} else if (is_category()) {
    siteTitle = __('categories') + ' - ' + page.category;
} else if (is_tag()) {
    siteTitle = __('tags') + ' - ' + page.tag;
}
%>
<title><% if (siteTitle){ %><%= __(siteTitle.toLowerCase()) %> - <% } %><%= config.title %><% if (is_home()){ %> - <%= config.subtitle %><% } %></title>
<%
var keywords;
if (page.keywords) {
    keywords = page.keywords;
} else {
    keywords = theme.keywords;
}
%>
<meta name="keywords" content="<%= keywords %>">
<%
var description;
if (page.description) {
    description = page.description;
} else {
    description = config.description;
}
%>
<meta name="description " content="<%= description %>">